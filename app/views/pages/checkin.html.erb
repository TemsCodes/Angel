<div class= "homepage_wrapper">
  <div class="homepage-text">
<h1>You're checked in!</h1>
</div>

<%= form_with url:send_message_path do |f| %>
  <%= f.submit "Check me!", id:"checkin" %>
  <% end %>

  <%= form_with url:send_call_path do |f| %>
  <%= f.submit "Call me", id:"callme" %>
  <% end %>

  <div>
    <button class="uk-button uk-button-default uk-button-large" id="finish" href="#">Date is finished</button>
</div>

